{
  "manifest_version": 3,
  "name": "StackOverflow Visit Counter",
  "description": "Check out how many times you have visited stackoverflow on a day",
  "version": "1.0.0",
  "author": "Jack Coder 25",
  "background": { "service_worker": "background.js" },
  "action": {
    "default_title": "StackOverflow Visits Today",
    "default_icon": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "96": "icons/96x96.png",
      "144": "icons/144x144.png",
      "192": "icons/192x192.png"
    }
  },
  "permissions": ["history"],
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "96": "icons/96x96.png",
    "144": "icons/144x144.png",
    "192": "icons/192x192.png"
  }
}
